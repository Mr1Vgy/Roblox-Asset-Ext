{
  "manifest_version": 2,

  "name": "Mr_Vgy's Roblox Asset Info",
  "description": "This extension show extra information about a Roblox asset",
  "version": "1.0",

  "browser_action": {
    "default_icon": "iconNew.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
	"https://api.roblox.com/"
  ]
}